n        setDonorName(e.target.value);\n    };\n\n    const handleDonationSubmit = (e) => {\n        e.preventDefault();\n        \n        // Example API call to submit the donation\n        axios.post('http://localhost:8080/api/donations', {\n            donorName: donorName,\n            amount: amount,\n        })\n        .then(response => {\n            console.log('Donation Successful', response.data);\n            setIsSuccess(true); // Update state on successful donation\n        })\n        .catch(error => {\n            console.error('There was an error submitting the donation!', error);\n            setIsSuccess(false); // Update state on error\n        });\n    };\n\n    return (\n        <div className=\"donate\">\n            <h1>Donate to Save Mother Nature</h1>\n            <p>Your contribution helps us protect the environment and promote sustainable practices.</p>\n            {isSuccess && <p>Thank you for your generous donation!</p>}\n            <form onSubmit={handleDonationSubmit}>\n                <label>Name:</label>\n                <input\n                    type=\"text\"\n                    name=\"donorName\"\n                    value={donorName}\n                    onChange={handleNameChange}\n                    required\n                />\n                <label>Enter Donation Amount:</label>\n                <input\n                    type=\"number\"\n                    name=\"amount\"\n                    value={amount}\n                    onChange={handleDonationChange}\n                    min=\"1\"\n                    required\n                />\n                <button type=\"submit\">Donate Now</button>\n            </form>\n        </div>\n    );\n}\n\nexport default Donate;\n","import React from 'react';\nimport { BrowserRouter as Router, Switch, Route } from 'react-router-dom';\nimport Navbar from './components/Navbar';\nimport Footer from './components/Footer';\nimport Home from './pages/Home';\nimport About from './pages/About';\nimport Contact from './pages/Contact';\nimport GetInvolved from './pages/GetInvolved';\nimport Learn from './pages/Learn';\nimport TakeAction from './pages/TakeAction';\nimport ExploreResources from './pages/ExploreResources';\nimport Donate from './pages/Donate';\n\nfunction App() {\n    return (\n        <Router>\n            <Navbar />\n            <Switch>\n                <Route exact path=\"/\" component={Home} />\n                <Route path=\"/about\" component={About} />\n                <Route path=\"/contact\" component={Contact} />\n                <Route path=\"/get-involved\" component={GetInvolved} />\n                <Route path=\"/learn\" component={Learn} />\n                <Route path=\"/take-action\" component={TakeAction} />\n                <Route path=\"/explore-resources\" component={ExploreResources} />\n                <Route path=\"/donate\" component={Donate} />\n            </Switch>\n            <Footer />\n        </Router>\n    );\n}\n\nexport default App;\n\n","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport './index.css';\nimport App from './App';\nimport { BrowserRouter as Router } from 'react-router-dom';\n\nReactDOM.render(\n  <Router>\n    <App />\n  </Router>,\n  document.getElementById('root')\n);\n"]}}